{
  "short_name": "Dao Chat",
  "name": "Dao Chat App",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "logo512.png",
    "48": "logo512.png",
    "128": "logo512.png"
  },
  "start_url": "/",
  "display": "fullscreen",
  "theme_color": "#00FFA3",
  "background_color": "#36393f"
}
