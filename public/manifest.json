{
  "short_name": "Dao Chat",
  "name": "Dao Chat",
  "description": "Decentralised chats for DAOs",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "orientation": "portrait",
  "permissions": ["storage"],
  "icons": [
    {
      "src": "splash_screens/icon.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "theme_color": "#00FFA3",
  "background_color": "#000000"
}
